      <script>#!/bin/bash
#shell options #-s enable (set) each optname #globstar enables ** recursive dir search
echo 'starting aws_data_test step';

#enable use of **
shopt -s globstar

#set header empty var to 0
headempty=0

#loop through txt files in ./drug/**/.txt and do
#cd ./drug/
cd ${BASE_FILE_DIR}/data_from_s3
curdir=`pwd`;
echo 'pwd is ';
echo `pwd`;
for i in drug demo; do # indi rpsr outc; do
    cd $i
    for name in ./**/*.txt; do
	echo 'i is '$i;
        if [ $headempty -eq 0 ]
        then
            #put header row into drug.txt
            head -1 $name --silent > ../$i.txt
            
            headempty=1 &amp;&amp;
            #tail outputs last -n+2 lines of a file #-q does not output the filename
            tail -n +2 --quiet $name >> ../$i.txt
        else
            #tail outputs last -n+2 lines of a file #-q does not output the filename
            tail -n +2 --quiet $name >> ../$i.txt
        fi    
    done;
    cd $curdir
done;
</script>
